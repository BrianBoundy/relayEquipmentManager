                "style": { "display": "block" },
                "labelAttrs": {
                  "style": { "width": "3rem" }
                },
                "columns": [
                  {
                    "hidden": true,
                    "binding": "name",
                    "text": "name",
                    "style": { "whiteSpace": "nowrap" }
                  },
                  {
                    "hidden": false,
                    "binding": "desc",
                    "text": "Description",
                    "style": { "whiteSpace": "nowrap" }
                  }
                ],
                "items": [
                  {
                    "name": "f",
                    "desc": "Fahrenheit"
                  },
                  {
                    "name": "c",
                    "desc": "Celcius"
                  },
                  {
                    "name": "k",
                    "desc": "Kelvin"
                  }
                ],
                "inputAttrs": { "style": { "width": "7rem" } }
              }
            },

            {
              "dataType": "panel",
              "field": {
                "type": "fieldset",
                "legend": "Device Info",
                "style": {
                  "display": "block",
                  "verticalAlign": "top"
                }
              },
